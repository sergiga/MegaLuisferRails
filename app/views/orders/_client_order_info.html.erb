<div id="client-search-info">
  <div class="row">
    <div class="col-xs-3", style="text-align:right;">
      Nombre:
    </div>
    <div class="col-xs-9", style="text-align:right;">
      <%= @client.name %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-3", style="text-align:right;">
      Dirección:
    </div>
    <div class="col-xs-9", style="text-align:right;">
      <%= @client.address %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-3", style="text-align:right;">
      Teléfono:
    </div>
    <div class="col-xs-9", style="text-align:right;">
      <%= @client.contact_phone %>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-3", style="text-align:right;">
      CIF/DNI:
    </div>
    <div class="col-xs-9", style="text-align:right;">
      <%= @client.cif_dni %>
    </div>
  </div>
</div>
